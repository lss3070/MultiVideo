{
    "manifest_version": 2,
    "name": "__MSG_appName__",
    "description": "__MSG_appDesc__",
    "default_locale":"en",
    "version": "0.1.3",
    
    "icons":{
        "16":"/image/multiview_16.png",
        "48":"/image/multiview_48.png",
        "128":"/image/multiview_128.png"
    },

    "permissions":[
        "webview", 
        "storage",
         "contextMenus",
          "alwaysOnTopWindows",
           "unlimitedStorage",
            "browser",
            "*://*.google.com/",
            "<all_urls>"
           
            ],   
    "app":{
        "background":{
            "scripts":["thirdparty/jquery-3.5.1.min.js","thirdparty/jquery-ui.js","background.js"],
            "persistent":false
        }
    },
    "minimum_chrome_version":"50"
}
